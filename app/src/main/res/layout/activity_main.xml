        android:ems="10"
        android:hint="Emergency contact person"
        android:inputType="textPersonName"
        android:paddingLeft="30dp"
        android:paddingTop="10dp"
        android:paddingRight="30dp"
        android:paddingBottom="10dp"
        android:textColor="@color/white"
